<p style="color: green"><%= notice %></p>

<h1>Properties</h1>

<div id="properties">
  <% @properties.each do |property| %>
    <%= render property %>
    <p>
      <%= link_to "More...", property, class: 'btn btn-success' %>
    </p>
  <% end %>
</div>

<%= link_to "New property", new_property_path, class: 'btn btn-primary' %>

<div id="<%= dom_id property %>">
  <div>
    <img src=<%= property.photo %> />
  </div>
  <p>
    <strong>Name:</strong>
    <%= property.name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= property.address %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= property.price %>
  </p>

  <p>
    <strong>Roomsinteger:</strong>
    <%= property.roomsinteger %>
  </p>


  <p>
    <strong>Account:</strong>
    <%= property.account_id %>
  </p>

</div>
